<?xml version="1.0" encoding="UTF-8"?> 
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
   
       
   <navigation-rule>
	   <from-view-id>/*</from-view-id>
	   <navigation-case>
		   <from-outcome>userconf</from-outcome>
		   <to-view-id>/admin/UserConfView.xhtml</to-view-id>
	   </navigation-case> 
   </navigation-rule>   
   
   
      <navigation-rule>
	   <from-view-id>/*</from-view-id>
	   <navigation-case>
		   <from-outcome>users</from-outcome>
		   <to-view-id>/admin/UserManagement.xhtml</to-view-id>
	   </navigation-case> 
   </navigation-rule>  
   
    <navigation-rule>
	   <from-view-id>/*</from-view-id>
	   <navigation-case>
		   <from-outcome>shop</from-outcome>
		   <to-view-id>http://localhost:8080/RS3/index.xhtml</to-view-id>
		   <redirect></redirect>
		   
	   </navigation-case> 
   </navigation-rule>  
   
    <navigation-rule>
	   <from-view-id>/*</from-view-id>
	   <navigation-case>
		   <from-outcome>loadproducts</from-outcome>
		   <to-view-id>http://localhost:8080/Lp/</to-view-id>
		   
	   </navigation-case> 
   </navigation-rule>  
   
    <navigation-rule>
	   <from-view-id>/*</from-view-id>
	   <navigation-case>
		   <from-outcome>shipments</from-outcome>
		   <to-view-id>http://localhost:8080/EShopProductDeliveryManagement/</to-view-id>
		   
	   </navigation-case> 
   </navigation-rule> 
      
    <navigation-rule>
	   <from-view-id>/*</from-view-id>
	   <navigation-case>
		   <from-outcome>maintenancetasks</from-outcome>
		   <to-view-id>/admin/UserManagement.xhtml</to-view-id>
		   
	   </navigation-case> 
   </navigation-rule> 
      
    <navigation-rule>
	   <from-view-id>/*</from-view-id>
	   <navigation-case>
		   <from-outcome>reports</from-outcome>
		   <to-view-id>/admin/UserManagement.xhtml</to-view-id>
		   
	   </navigation-case> 
   </navigation-rule> 
 
</faces-config>